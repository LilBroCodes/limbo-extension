{
    "name": "Limbo Language",
    "scopeName": "source.limbo",
    "fileTypes": ["limbo"],
    "patterns": [
      {
        "name": "keyword.limbo",
        "match": "^(name|version|minecraft_version|description|output|namespace|pack_format|target_path|author|pack_icon|tags|blocks|items|functions|predicates|path):"
      },
      {
        "name": "string.quoted.double.limbo",
        "match": "\"[^\"\\n]*\""
      },
      {
        "name": "constant.numeric.limbo",
        "match": "\\b\\d+(\\.\\d+)?\\b"
      },
      {
        "name": "entity.name.function.limbo",
        "match": "\\b(load|tick)\\b"
      },
      {
        "name": "variable.other.limbo",
        "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
      },
      {
        "name": "comment.line.number-sign.limbo",
        "match": "#.*$"
      }
    ],
    "repository": {}
  }
  